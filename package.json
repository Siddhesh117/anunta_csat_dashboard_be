{
    "name": "atrs_be",
    "version": "1.0.0",
    "description": "Backend Software for ATRS",
    "main": "server.js",
    "scripts": {
        "format:check": "prettier --check ./src/*",
        "format:write": "prettier --write ./src/*",
        "lint:check": "eslint --no-error-on-unmatched-pattern .",
        "lint:fix": "eslint --no-error-on-unmatched-pattern --fix .",
        "tsc-watch": "tsc --w",
        "tsc": "tsc",
        "dev": "cross-env NODE_ENV=development nodemon dist/server",
        "start": "cross-env NODE_ENV=development node dist/server",
        "start-uat": "cross-env NODE_ENV=uat node dist/server",
        "dev-migrate-up": "cross-env NODE_ENV=development sequelize db:migrate",
        "dev-migrate-down": "cross-env NODE_ENV=development sequelize db:migrate:undo",
        "dev-migrate-down-all": "cross-env NODE_ENV=development sequelize db:migrate:undo:all",
        "test": "mocha --require ts-node/register src/tests/**/*.test.ts",
        "encrypt-data": "cross-env NODE_ENV=development node dist/scripts/migrateOldData"
    },
    "keywords": [],
    "author": "Coreco Technologies Pvt Ltd.",
    "license": "ISC",
    "dependencies": {
        "@types/multer": "^1.4.7",
        "axios": "^1.6.7",
        "bcrypt": "^5.1.0",
        "body-parser": "^1.20.2",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "generate-password": "^1.7.0",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.0",
        "moment-timezone": "^0.5.43",
        "multer": "^1.4.5-lts.1",
        "mysql2": "^3.3.1",
        "nodemailer": "^6.9.3",
        "otp-generator": "^4.0.1",
        "path": "^0.12.7",
        "sequelize": "^6.31.1",
        "urlencode": "^2.0.0",
        "uuid": "^9.0.1",
        "winston": "^3.8.2",
        "winston-daily-rotate-file": "^4.7.1",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cookie-parser": "^1.4.6",
        "@types/cors": "^2.8.13",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.17",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/node": "^20.2.1",
        "@types/nodemailer": "^6.4.8",
        "@types/superagent": "^4.1.18",
        "@types/uuid": "^9.0.4",
        "@typescript-eslint/eslint-plugin": "^5.59.6",
        "@typescript-eslint/parser": "^5.59.6",
        "cross-env": "^7.0.3",
        "eslint": "^8.40.0",
        "eslint-config-prettier": "^8.8.0",
        "nodemon": "^3.0.3",
        "pg": "^8.11.0",
        "prettier": "^2.8.8",
        "sequelize-cli": "^6.6.0",
        "sonarqube-scanner": "^3.1.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.4"
    }
}
